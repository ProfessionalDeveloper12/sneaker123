<div class="section-standard hero lp-template">
    <div class="flex-horizontal flip-svp">
      <div class="col small">
        <div class="banner-left">
          <h1 class="heading">Find your next sneaker</h1>
          <p class="subheadline">Search over 60,000 sneakers from consignment stores and marketplaces around the world</p>
          <div class="form-block w-form">
            <form class="form" action="{{ routes.search_url }}" name="GET" role="search">
              <input type="text" class="text-field w-input" maxlength="256" name="q" placeholder="Your next sneaker...">
              <input type="submit" class="button-3 w-button" value="Search" >
            </form>
            <div class="w-form-done">
              <div>Thank you! Your submission has been received!</div>
            </div>
            <div class="w-form-fail">
              <div>Oops! Something went wrong while submitting the form.</div>
            </div>
          </div>
        </div>
      </div>
      <div class="spacer-60 _60-width"></div>
      <div class="flex-horizontal">
        <div class="marquee">
